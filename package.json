{
  "name": "riverbed",
  "version": "1.0.0",
  "license": "AGPL-3.0-or-later",
  "scripts": {
    "start": "webpack server --config webpack.dev.js",
    "test": "jest --watchAll",
    "test:ci": "jest",
    "cypress": "cypress open",
    "cypress:run": "cypress run",
    "format": "npx prettier src cypress/e2e --write",
    "lint:eslint": "eslint src cypress/e2e  --max-warnings=0",
    "lint:format": "npx prettier src cypress/e2e --check",
    "lint": "npm run lint:eslint && npm run lint:format",
    "lint:ci": "npm run lint",
    "build": "webpack --config webpack.prod.js",
    "explore:app": "source-map-explorer dist/app.*.js",
    "explore:vendors": "source-map-explorer dist/vendors.*.js",
    "deploy": "npm run build && netlify deploy --prod",
    "prepare": "husky"
  },
  "dependencies": {
    "@codingitwrong/jsonapi-client": "^0.0.11",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^6.4.8",
    "@mui/material": "^6.4.8",
    "@mui/x-date-pickers": "^7.28.3",
    "@tanstack/react-query": "^4.29.5",
    "dayjs": "^1.11.13",
    "google-maps-react": "^2.0.6",
    "jest": "^29.7.0",
    "lodash": "^4.17.21",
    "react": "18.3.1",
    "react-beautiful-dnd": "^13.1.1",
    "react-beforeunload": "^2.6.0",
    "react-dom": "18.3.1",
    "react-native": "0.71.7",
    "react-native-web": "~0.18.9",
    "react-router-dom": "^7.5.2",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/eslint-parser": "^7.27.0",
    "@babel/plugin-proposal-private-methods": "^7.18.6",
    "@babel/preset-env": "^7.26.9",
    "@babel/preset-react": "^7.26.3",
    "@tanstack/react-query-devtools": "^4.29.6",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^15.0.7",
    "@testing-library/user-event": "^14.6.1",
    "babel-loader": "^10.0.0",
    "copy-webpack-plugin": "^13.0.0",
    "cypress": "^14.3.1",
    "dotenv": "^16.5.0",
    "eslint": "^8.57",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-cypress": "^3.6.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jest": "^28.11.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "html-webpack-plugin": "^5.6.3",
    "husky": "^9.1.7",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "^3.5.3",
    "source-map-explorer": "^2.5.3",
    "webpack": "^5.99.6",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.1",
    "webpack-merge": "^6.0.1"
  },
  "private": true,
  "jest": {
    "testEnvironment": "jsdom",
    "modulePathIgnorePatterns": [
      "cypress"
    ],
    "setupFilesAfterEnv": [
      "./setupTests.js"
    ]
  }
}
